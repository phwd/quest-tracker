    <string name="invite_friends_header">Kutsu kavereita</string>
    <string name="leave_party_dlg_header">Poistutaanko ryhmästä?</string>
    <string name="leave_party_dlg_desc">Tähän ryhmään on vielä hyväksymättömiä kutsuja. Jos poistut nyt, kaikki tämän ryhmän kutsut peruutetaan.</string>
