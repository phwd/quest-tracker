            <TextView android:id="@+id/assistant_in_app_text" android:text="Notification Text goes here. It can be multiple lines." style="@style/AssistantInAppToastText"/>
        </LinearLayout>
    </com.oculus.notifications.view.assistant.NotificationAssistantView>
</FrameLayout>
