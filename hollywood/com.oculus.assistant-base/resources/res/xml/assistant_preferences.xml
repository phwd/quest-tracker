<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">
    <ListPreference android:entries="@array/server_types" android:title="@string/overrideServerEndpoint_title" android:key="@string/overrideServerEndpoint" android:defaultValue="PRODUCTION" android:entryValues="@array/server_types_values"/>
    <EditTextPreference android:title="@string/voice_interaction_endpoint_title" android:key="@string/voice_interaction_endpoint"/>
    <SwitchPreference android:title="@string/use_override_endpoint_title" android:key="@string/use_override_endpoint"/>
