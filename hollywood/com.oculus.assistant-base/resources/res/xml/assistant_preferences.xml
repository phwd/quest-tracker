        <ListPreference android:entries="@array/mc_bool_override" android:title="Disable In-Game Voice Commands" android:key="mc_ingame_voice_commands_disabled" android:entryValues="@array/mc_bool_override_values"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Use Native voice service" android:key="mc_use_native_voice_service" android:entryValues="@array/mc_bool_override_values"/>
