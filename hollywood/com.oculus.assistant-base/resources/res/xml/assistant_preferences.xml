        <SwitchPreference android:title="Enable Override" android:key="enable_mc_override"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Enable OACR" android:key="mc_enable_oacr" android:entryValues="@array/mc_bool_override_values"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Disable NS" android:key="mc_disable_noise_supression" android:entryValues="@array/mc_bool_override_values"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Don&apos;t Re-enable NS" android:key="mc_dont_reenable_noise_suppression" android:entryValues="@array/mc_bool_override_values"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Enable Audio Ducking" android:key="mc_audio_ducking_eabled" android:entryValues="@array/mc_bool_override_values"/>
        <ListPreference android:entries="@array/mc_bool_override" android:title="Enable Server Side &apos;What Can I Do&apos;" android:key="mc_server_side_what_can_i_do" android:entryValues="@array/mc_bool_override_values"/>
