<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:height="24dp" android:width="24dp">
        <bitmap android:src="@drawable/oc_icon_settings_filled_24_d2d2d2"/>
    </item>
    <item>
        <selector>
            <item android:state_selected="true">
                <shape>
                    <corners android:radius="@dimen/messenger_tablet_image_bubble_min_height"/>
