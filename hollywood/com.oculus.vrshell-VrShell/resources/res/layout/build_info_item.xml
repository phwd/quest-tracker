<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="horizontal" android:padding="2dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:layout_width="150dp" android:layout_height="wrap_content" android:layout_marginRight="8dp">
        <TextView android:textSize="16sp" android:textColor="@color/deprecated_oc_white_fix_me" android:layout_gravity="center_vertical" android:id="@+id/dogfood_build_info_app_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="15dp"/>
        <ImageButton android:layout_gravity="center_vertical" android:id="@+id/dogfood_open_tab_button" android:background="@drawable/open_tab_image" android:layout_width="10dp" android:layout_height="10dp" android:layout_marginRight="8dp" android:layout_centerVertical="true"/>
    </LinearLayout>
    <TextView android:textSize="16sp" android:textColor="@color/deprecated_oc_gray_6_fix_me" android:id="@+id/dogfood_build_info_version_code" android:layout_width="100dp" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:layout_centerVertical="true"/>
    <TextView android:textSize="16sp" android:textColor="@color/deprecated_oc_gray_6_fix_me" android:id="@+id/dogfood_build_info_version_name" android:layout_width="200dp" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:singleLine="true" android:layout_centerVertical="true"/>
    <TextView android:textSize="10sp" android:textColor="@color/deprecated_oc_gray_6_fix_me" android:id="@+id/dogfood_build_info_status" android:layout_width="100dp" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:singleLine="false" android:layout_centerVertical="true"/>
</LinearLayout>
