<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:ordering="sequentially">
    <set android:ordering="together">
        <objectAnimator android:duration="0" android:valueFrom="1" android:valueTo="1" android:propertyName="scaleX"/>
        <objectAnimator android:duration="0" android:valueFrom="1" android:valueTo="1" android:propertyName="scaleY"/>
        <objectAnimator android:duration="0" android:valueFrom="0" android:valueTo="0" android:propertyName="translateX"/>
        <objectAnimator android:duration="0" android:valueFrom="0" android:valueTo="0" android:propertyName="translateY"/>
    </set>
    <objectAnimator android:duration="900" android:valueFrom="0" android:valueTo="0" android:propertyName="translateX"/>
    <set android:ordering="together">
        <objectAnimator android:duration="400" android:valueFrom="0.75" android:valueTo="1" android:propertyName="scaleX"/>
        <objectAnimator android:duration="400" android:valueFrom="0.75" android:valueTo="1" android:propertyName="scaleY"/>
    </set>
    <objectAnimator android:duration="1000" android:valueFrom="0" android:valueTo="0" android:propertyName="translateX"/>
    <set android:ordering="together">
        <objectAnimator android:duration="300" android:valueFrom="0" android:valueTo="-230" android:propertyName="translateX"/>
        <objectAnimator android:duration="300" android:valueFrom="0" android:valueTo="70" android:propertyName="translateY"/>
    </set>
</set>
